{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "strict": false,
    "declaration": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "removeComments": false,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "moduleResolution": "Node",
    "lib": ["es2015", "dom"],
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
